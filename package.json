{
  "name": "react-ssr",
  "version": "1.0.0",
  "description": "Server side rendering project",
  "main": "src/index.js",
  "scripts": {
    "clean": "rm -rf build && rm -rf public && rm -rf node_modules",
    "dev": "npm-run-all --parallel dev:*",
    "dev:server": "nodemon --watch build --exec \"node build/bundle.js\"",
    "dev:build-server": "webpack --config ./webpack/webpack.server.js --watch",
    "dev:build-client": "webpack --config ./webpack/webpack.client.js --watch",
    "prod:start": "npm run prod:setup && npm run prod:server",
    "prod:server": "node build/bundle.js",
    "prod:setup": "npm install && npm-run-all --parallel prod:build-*",
    "prod:build-server": "webpack --config ./webpack/webpack.server.js",
    "prod:build-client": "webpack --config ./webpack/webpack.client.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@loadable/component": "^5.10.1",
    "@loadable/server": "^5.9.0",
    "axios": "0.16.2",
    "babel-cli": "^6.26.0",
    "babel-loader": "^8.0.6",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "compression": "1.7.0",
    "concurrently": "3.5.0",
    "express": "4.15.4",
    "express-http-proxy": "1.0.6",
    "lodash": "4.17.21",
    "nodemon": "1.12.0",
    "npm-run-all": "4.1.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "5.2.0",
    "react-redux": "5.0.6",
    "react-router-config": "1.0.0-beta.4",
    "react-router-dom": "4.2.2",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0",
    "serialize-javascript": "1.4.0",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.5.0",
    "@babel/preset-env": "^7.5.2",
    "@babel/preset-react": "^7.0.0",
    "@loadable/babel-plugin": "^5.10.0",
    "@loadable/webpack-plugin": "^5.7.1"
  }
}
